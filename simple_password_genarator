
import random
def password_genarator():
  
  try:
    chars='abcdefghijklmnopqrstuvwxyz0123456789@!?.,-'
    chars.upper()
    choose=int(input("Enter length of your password:" "\n"))
    passwords=''

    for password in range(choose):
      passwords+=random.choice(chars)
    print(passwords)
  except ValueError:
      print("Enter Valid Length Number.Enter  Only digit ")
  finally:
      print("Thanks")
password_genarator()
